{
    "name": "blackoutzz/framework",
    "description": "Core Framework to deploy full fledge php application",
    "version": "1.0.0",
    "homepage": "https://github.com/Blackoutzz-Engineering/php-framework",
    "keywords": ["php" , "mvc" , "components" , "framework" , "app"],
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Mickael Nadeau",
            "homepage": "https://github.com/Blackoutzz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "core\\": "src/core"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-dev": "1.0.0-dev"
        }
    }
}
